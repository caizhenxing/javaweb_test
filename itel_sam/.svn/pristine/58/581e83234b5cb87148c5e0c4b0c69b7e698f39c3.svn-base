function NewPanel(o) {
	 if(o=='M1-0'){
		 return DeviceManager(o);
	 }else if(o=='M1-1'){
		 return UpgradePanel(o);
	 }else if(o=='M1-2'){
		 return AndrodHelpCenterPanel(o);
	 }else if(o=='M1-3'){
		 return IOSHelpCenterPanel(o);
	 }else if(o=='M1-4'){
		 return SamDeviceManager(o);
	 }
	 
}