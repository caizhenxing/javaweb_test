	<style type="text/css">
		body {
			font-family:'lucida grande',tahoma,arial,sans-serif;
			font-size:11px;
		}
		a {
			color:#15428B;
		}
		a:link, a:visited {
			text-decoration: none;
		}
		a:hover {
			text-decoration: underline;
		}
		#header {
			background: #7F99BE url(${cts}/images/top-right.gif) repeat-x center;
		}
		#header div {
			font-size: 16px;
			color: #fff;
			font-weight: normal;
			padding: 5px 10px;
		}
		.x-grid-cell-topic b {
            display: block;
        }
        .x-grid-cell-topic .x-grid-cell-inner {
            white-space: normal;
        }
        .x-grid-cell-topic a {
            color: #385F95;
            text-decoration: none;
        }
        .x-grid-cell-topic a:hover {
            text-decoration:underline;
        }
		.x-grid-cell-topic .x-grid-cell-innerf {
			padding: 5px;
		}
		.x-grid-rowbody {
	        padding: 0 5px 5px 5px; 
		}
		.feed-list {
			padding: 0 3px 0 3px;
		}
		
		.feed-list-item {
			margin-top: 3px;
			padding-left: 20px;
			font-size: 11px;
			line-height: 20px;
			cursor: pointer;
			background: url(./images/rss.gif) no-repeat 0 2px;
			border: 1px solid #fff;
		}
		
		.feed-list .x-item-selected {
			font-weight: bold; 
			color: #15428B;
			background-color: #DFE8F6;
			border: 1px dotted #A3BAE9;
		}
		
		.feed-list-item-hover {
			background-color: #eee;
		}

